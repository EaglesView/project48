{
  "name": "BlueJay",
  "tree": {
    "$className": "DataModel",

    "ReplicatedFirst":{
      "Source":{
        "$path":"src/init",
        "$ignoreUnknownInstances": true
      }
    },

    "ReplicatedStorage": {
      "Source":{
        "$path": "src/shared",
        "$ignoreUnknownInstances": true
      }
    },

    "ServerScriptService": {
        "$path": "src/server/ServerScriptService"
    },

    "ServerStorage":{
      "Source":{
        "$className" : "Folder",
        "$path": "src/server/ServerStorage"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Player" :{
          "$path": "src/StarterPlayer/StarterPlayer"

        }
    
      },
      "StarterCharacterScripts":{
        "Character": {
            "$path" : "src/StarterPlayer/StarterCharacter"
        }
      }
    },

    "Workspace": {
      "CharacterPaths":{
        "$className": "Folder",
        "$path": "src/wrkSpc/char",
        "$ignoreUnknownInstances": true
      },
      "$properties": {
        "FilteringEnabled": true
      },
      "Baseplate": {
        "$className": "Part",
        "$properties": {
          "Anchored": true,
          "Color": [
            0.38823,
            0.37254,
            0.38823
          ],
          "Locked": true,
          "Position": [
            0,
            -10,
            0
          ],
          "Size": [
            512,
            20,
            512
          ]
        }
      }
    },
    "Lighting": {
      "$properties": {
        "Ambient": [
          0,
          0,
          0
        ],
        "Brightness": 1,
        "GlobalShadows": true,
        "Outlines": true,
        "Technology": "Future"
      }
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}